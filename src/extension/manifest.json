{
    "manifest_version": 3,
    "name": "Linx",
    "description": "Save a bookmark to Linx.",
    "version": "0.9",
    "options_page": "settings.html",
    
    "action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "storage"
    ],
    "host_permissions": [
        "https://*/*"
    ]
}
